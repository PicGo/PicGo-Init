{
  "name": "picgo-init",
  "type": "module",
  "version": "1.0.0",
  "description": "Standalone initializer for PicGo plugin templates.",
  "author": {
    "name": "Molunerfinn",
    "email": "marksz@teamsz.xyz"
  },
  "license": "MIT",
  "homepage": "https://github.com/PicGo/picgo-init#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PicGo/picgo-init.git"
  },
  "bugs": {
    "url": "https://github.com/PicGo/picgo-init/issues"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs",
    "./package.json": "./package.json"
  },
  "bin": {
    "picgo-init": "./dist/cli.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">= 22"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "check": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^13.1.0",
    "ejs": "^3.1.10",
    "fs-extra": "^11.3.1",
    "giget": "^2.0.0",
    "globby": "^11.1.0",
    "inquirer": "^12.9.0",
    "minimatch": "^3.1.2"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^25.0.3",
    "bumpp": "^10.3.2",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}